<!--
 document type declaration (DTD) for the SEP SeaFaults mapping 
-->
<!--  Sem2, 2016:  -->
<!--  General Attributes definition  -->
<seafaultsmap units="centimetres">
<attribute>
<key>Survey Date</key>
<value>01/01/2016</value>
</attribute>
<attribute>
<key>Robot Model</key>
<value>Lego Mindstorm EV3</value>
</attribute>
<boundary>
<area>
<point x="0" y="0"/>
<point x="60" y="0"/>
<point x="60" y="50"/>
<point x="0" y="50"/>
</area>
</boundary>
<robot-status>
<point x="16" y="18"/>
<attribute>
<key>heading</key>
<!--  in degrees  -->
<value>90</value>
<!--  0 is north, increasing CW  -->
</attribute>
</robot-status>
<container>
<point x="20" y="10"/>
</container>
<obsticle>
<point x="19" y="20"/>
</obsticle>
 <obsticle>
  <point x="19" y="20"/>
 </obsticle>
 <obsticle>
  <point x="5" y="3"/>
 </obsticle>
 <obsticle>
  <point x="59" y="49"/>
 </obsticle>
<depth-to-color>
<attribute>
<key>1</key>
<!--  depth in increasing order  -->
<value>0000FF</value>
<!--  blue  -->
</attribute>
<attribute>
<key>2</key>
<value>FF0000</value>
<!--  red  -->
</attribute>
<attribute>
<key>3</key>
<value>00FF00</value>
<!--  green  -->
</attribute>
</depth-to-color>
<zone state="explored">
<area>
<point x="20" y="35"/>
<point x="0" y="35"/>
<point x="0" y="18"/>
<point x="16" y="18"/>
<point x="16" y="20"/>
<point x="20" y="20"/>
</area>
</zone>
<zone state="nogo">
<circle x="16" y="16" radius="2"/>
</zone>
<zone state="nogo">
<area>
<point x="10" y="33"/>
<point x="4" y="33"/>
<point x="6" y="31"/>
<point x="6" y="28"/>
<point x="10" y="28"/>
</area>
</zone>
<zone state="unexplored">
<area>
<point x="10" y="33"/>
<point x="3" y="33"/>
<point x="4" y="27"/>
<point x="10" y="27"/>
</area>
</zone>
<fault-line>
<point x="19" y="25"/>
<point x="17" y="25"/>
<point x="15" y="27"/>
<attribute>
<key>depth</key>
<value>1</value>
</attribute>
</fault-line>
<fault-line>
<point x="15" y="20"/>
<point x="13" y="22"/>
<point x="12" y="24"/>
<point x="10" y="30"/>
<point x="10" y="33"/>
