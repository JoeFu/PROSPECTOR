<key>depth</key>
<value>3</value>
</attribute>
</fault-line>
<fault-line>
<point x="10" y="18.6"/>
<point x="10.5" y="18.3"/>
<point x="11" y="18.2"/>
<point x="11.5" y="18"/>
<point x="12" y="18.2"/>
<point x="12.5" y="18.3"/>
<point x="13" y="18.5"/>
<point x="13.5" y="19"/>
<point x="14" y="19.5"/>
<point x="14.5" y="20"/>
<attribute>
<key>depth</key>
<value>2</value>
</attribute>
</fault-line>
</seafaultsmap>